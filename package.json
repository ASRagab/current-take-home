{
  "name": "current",
  "version": "1.0.0",
  "description": "take home project for current",
  "main": "server.js",
  "scripts": {
    "build": "tsc",
    "start:server": "node ./dist/server.js",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint:prettier": "prettier --find-config-path --check '**/*.{graphql,js,json,md,ts,yaml.yml}'",
    "lint": "npx npm-run-all -p 'lint:*'",
    "cloud-sql-proxy": "./cloud_sql_proxy -instances=oregonstate-dev-261504:us-east4:current-primary=tcp:5432 &",
    "seed": "psql -h 127.0.0.1 -d current --user current -c \"\\copy seed FROM 'transactions.csv' DELIMITER ',' CSV HEADER;\"",
    "dev": "run-p lint:* && npm run build && npm run start:watch | bunyan",
    "start:watch": "nodemon --signal SIGTERM --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
    "test": "jest --verbose --detectOpenHandles"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ASRagab/current-take-home.git"
  },
  "author": "Ahmad Ragab",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ASRagab/current-take-home/issues"
  },
  "homepage": "https://github.com/ASRagab/current-take-home#readme",
  "dependencies": {
    "@googlemaps/google-maps-services-js": "3.1.16",
    "bunyan": "1.8.12",
    "compression": "1.7.4",
    "date-fns": "^2.9.0",
    "deepmerge": "4.2.2",
    "express": "^4.17.1",
    "express-basic-auth": "1.2.0",
    "fp-ts": "^2.1.2",
    "knex": "0.21.1",
    "morgan": "1.10.0",
    "npm-run-all": "^4.1.5",
    "objection": "2.2.16",
    "pg": "8.2.1",
    "uuid": "^7.0.1"
  },
  "devDependencies": {
    "@types/bunyan": "1.8.6",
    "@types/compression": "1.7.0",
    "@types/express": "^4.17.2",
    "@types/jest": "^26.0.18",
    "@types/morgan": "1.9.1",
    "@types/node": "^12.12.11",
    "@types/supertest": "2.0.9",
    "@types/uuid": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.3.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.15.2",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^26.6.0",
    "mock-knex": "0.4.9",
    "nodemon": "2.0.4",
    "prettier": "^1.18.2",
    "prettier-eslint": "^9.0.0",
    "supertest": "4.0.2",
    "ts-jest": "^26.0.2",
    "ts-node": "^8.5.4",
    "tsc-watch": "^4.0.0",
    "typescript": "^3.8.3"
  }
}
